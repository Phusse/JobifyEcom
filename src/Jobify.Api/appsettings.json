{
  "ConnectionStrings": {
    "Database": "Server=<YOUR_DB_SERVER>;Database=JobifyDb;User Id=<YOUR_DB_USER>;Password=<YOUR_DB_PASSWORD>;TrustServerCertificate=True;",
    "Redis": "<YOUR_REDIS_HOST_AND_PORT>[, password=<YOUR_REDIS_PASSWORD_IF_ANY>]"
  },
  "DataEncryption": {
    "CurrentKeyVersion": 1,
    "Keys": {
      "1": "<ENCRYPTION_KEY_BASE64>"
    }
  },
  "Hashing": {
    "EmailHmacKey": "<YOUR_SECRET_HMAC_KEY>",
    "PasswordWorkFactor": 12
  },
  "SessionManagement": {
    "StandardSessionDurationHours": 8,
    "ExtendedSessionDurationDays": 7,
    "AbsoluteSessionLimitDays": 90,
    "StandardExpiryExtensionHours": 1,
    "ExtendedExpiryExtensionHours": 12,
    "ExpiryExtensionTriggerPercent": 5
  },
  "SessionSigningKey": "<YOUR_SECRET_HMAC_KEY>",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "ecom": {
        "ClusterId": "ecom",
        "Match": {
          "Path": "/api/ecom/{**catch-all}"
        }
      },
      "matching": {
        "ClusterId": "matching",
        "Match": {
          "Path": "/api/matching/{**catch-all}"
        }
      },
      "matching-ai": {
        "ClusterId": "matching-ai",
        "Match": {
          "Path": "/api/matching-ai/{**catch-all}"
        }
      },
      "roadmap": {
        "ClusterId": "roadmap",
        "Match": {
          "Path": "/api/roadmap/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "ecom": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:5122/"
          }
        }
      },
      "matching": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:7001/"
          }
        }
      },
      "matching-ai": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:7002/"
          }
        }
      },
      "roadmap": {
        "Destinations": {
          "d1": {
            "Address": "http://localhost:7003/"
          }
        }
      }
    }
  }
}